{
  "expo": {
    "name": "הבנק הראשון שלי",
    "slug": "my-first-bank",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#363749"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.tsahiBarshavsky.myFirstBank",
      "buildNumber": "1.0.0"
    },
    "android": {
      "package": "com.tsahiBarshavsky.myFirstBank",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "splash": {
        "statusBar": {
          "barStyle": "light-content"
        }
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
